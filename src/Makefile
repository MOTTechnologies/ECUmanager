	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr//share/pixmaps/ecumanager-icon.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr//share/pixmaps/ 


install:  install_target install_menu_entry install_ecum_icon  FORCE

uninstall: uninstall_target uninstall_menu_entry uninstall_ecum_icon   FORCE

FORCE:

